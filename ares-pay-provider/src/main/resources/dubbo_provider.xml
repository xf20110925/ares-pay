<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	 http://code.alibabatech.com/schema/dubbo	http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       default-lazy-init="false" default-autowire="byName">

    <description>dubbo provider配置</description>

    <dubbo:application name="ptb-pay" />

    <dubbo:registry address="${dubbo.registry.address}" protocol="zookeeper" id="payProvider" />

    <dubbo:protocol name="dubbo" port="${dubbo.port}" dispather="all" threadpool="cached" threads="5000"/>

    <dubbo:service interface="com.ptb.pay.api.ITestApi"
                   ref="testApi"
                   registry="payProvider"
                   loadbalance="roundrobin"
                   owner="zuokui.fu"
                   version="1.0" />

</beans>